<h1>Movie List</h1>
<hr>
<table>
	<thead>
		<tr>
			<th>ID</th>
			<th>Nombre</th>
			<th>Director</th>
			<th>Sinopsis</th>
		</tr>
	</thead>
	<tbody>
	<% @movies_all.each do |movie| %>
		<tr>
			<td><%= movie.id %></td>
			<td><%= movie.name %></td>
			<td><%= movie.director %></td>
			<td><%= movie.synopsis %></td>
		</tr>
	<% end %>
	</tbody>
</table>
<hr>
